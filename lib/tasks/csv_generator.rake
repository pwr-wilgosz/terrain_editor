  desc "Create random map and export it to file. Run it by i.e. `rake csv:random['output.csv',30]`"
  task :random, [:filename, :seed] => :environment do |t, args|
    args.with_defaults(filename: "output.csv", seed: 300)
    #First create new map with random points based of existing set to create new set of edges
    seed = (args[:seed] || '0').to_i
    m = Map.first
    points = m.points.select(:x, :y, :z).reduce([]) do |r, point|
      newx = point.x + Random.rand(2*seed+1)-seed  # pointX + range (-seed, seed)
      newy = point.y + Random.rand(2*seed+1)-seed  # pointY + range (-seed, seed)
      newz = point.z + Random.rand(2*seed+1)-seed  # pointZ + range (-seed, seed)
      r << { x: newx, y: newy, z: point.z }
    end

    new_map = Map.create(name: "Some name")
    map_id = new_map.id
    point_objects = new_map.points.new(points)

